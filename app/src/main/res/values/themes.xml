<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <!--<style name="Theme.TeethHistory" parent="Theme.MaterialComponents.DayNight.DarkActionBar">-->
    <style name="Theme.TeethHistory" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/blue_700</item>
        <item name="colorPrimaryVariant">@color/blue_700_dark</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/deep_orange_400</item>
        <item name="colorSecondaryVariant">@color/deep_orange_400_dark</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->

        <item name="colorEventsListHolderPressed">@color/holder_events_list_background_pressed</item>
        <item name="colorEventsListHolderSelected">@color/holder_events_list_background_selected</item>
        <item name="colorEventsListHolderUnselected">@color/holder_events_list_background_unselected</item>

        <item name="android:colorBackground">@color/grey_50</item>

    </style>

</resources>